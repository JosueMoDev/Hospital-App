{
  "name": "clinic-node-api",
  "version": "1.0.0",
  "main": "app.ts",
  "repository": "https://github.com/JosueMoDev/clinic-node-api.git",
  "author": "Jonas Morales <josuemodev@gmail.com>",
  "scripts": {
    "seed": "npx tsnd --clear src/db/seeder.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsnd --respawn --clear --env-file=.env -r tsconfig-paths/register src/app.ts",
    "build": "rimraf ./dist && tsc",
    "start": "yarn run build && node --env-file=.env -r module-alias/register dist/app.js",
    "test": " yarn run jest",
    "test:watch": " yarn run  jest --watch",
    "test:coverage": "yarn run jest --coverage"
  },
  "_moduleAliases": {
    "@domain/datasources": "dist/domain/datasources",
    "@domain/dtos": "dist/domain/dtos",
    "@domain/entities": "dist/domain/entities",
    "@domain/repositories": "dist/domain/repositories",
    "@domain/use-cases": "dist/domain/use-cases",
    "@infraestructure/datasourcesimpl": "dist/infraestructure/datasources",
    "@infraestructure/repositoriesimpl": "dist/infraestructure/repositories",
    "@handler-errors": "dist/domain/errors",
    "@app": "dist/presentation/app",
    "@presentation": "dist/presentation",
    "@middlewares": "dist/presentation/middlewares",
    "@config": "dist/config",
    "@api-documentation": "dist/presentation/doc"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express-fileupload": "^1.4.4",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.7",
    "google-auth-library": "^9.2.0",
    "jest": "^29.7.0",
    "prisma": "5.5.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "@types/compression": "^1.7.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.1",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "env-var": "^7.4.1",
    "eslint": "9.x",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "openapi-types": "^12.1.3",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.13.1"
  }
}
